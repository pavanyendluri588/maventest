pipeline{
agent any
stages{
stage("build"){
steps{

sh 'mvn deploy'
}
}
stage("test"){
steps{
sh 'echo "test stage started"'
}
}
stage("deploying  build file to github"){
steps{
sh "git init"
sh "git add Target/*"
sh "git remote add orign https://github.com/pavanyendluri588/maventest/edit/master/.git "
sh "git commit -m "push2""
sh "git push -u orign master"
}
}
}
}
